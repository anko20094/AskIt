{
  "name": "ask-it",
  "private": true,
  "dependencies": {
    "@hotwired/turbo-rails": "^7.3.0",
    "@popperjs/core": "^2.11.7",
    "@rails/actioncable": "^7.0.4-3",
    "@rails/activestorage": "^7.0.4-3",
    "autoprefixer": "^10.4.14",
    "bootstrap": "^5.2.3",
    "bootstrap-icons": "^1.10.3",
    "esbuild": "^0.17.15",
    "sass": "^1.60.0",
    "tom-select": "^2.2.2"
  },
  "version": "0.1.0",
  "browserslist": [
    "defaults"
  ],
  "scripts": {
    "build:css:dev": "sass --style compressed ./app/assets/stylesheets/application.bootstrap.scss ./app/assets/builds/application.css --no-source-map --load-path=node_modules",
    "build:css": "sass ./app/assets/stylesheets/application.bootstrap.scss:./app/assets/builds/application.css --no-source-map --load-path=node_modules",
    "build": "esbuild app/javascript/*.* --bundle --sourcemap --outdir=app/assets/builds --public-path=assets"
  },
  "packageManager": "yarn@3.5.0"
}
